{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript",
    "jest"
  ],
  "plugins": [
    ["@babel/plugin-transform-typescript"],
    [
      "@babel/plugin-proposal-decorators",
      {
        "version": "2023-01"
      }
    ],
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "@": "./src",
          "@test": "./test"
        }
      }
    ]
  ]
}
