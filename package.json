{
  "name": "express-beans",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/example/app.ts -w src",
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "start:prod": "nodemon server.js",
    "test": "jest",
    "coverage": "vitest run --coverage",
    "pretest:prod": "npm run build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/express-beans.cjs.js",
  "module": "./dist/express-beans.es.js",
  "exports": {
    ".": {
      "import": "./dist/express-beans.es.js",
      "require": "./dist/express-beans.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.3.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0"
  }
}
