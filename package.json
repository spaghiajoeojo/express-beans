{
  "name": "express-beans",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/example/app.ts -w src",
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "start:prod": "nodemon server.js",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/express-beans.cjs.js",
  "module": "./dist/express-beans.es.js",
  "exports": {
    ".": {
      "import": "./dist/express-beans.es.js",
      "require": "./dist/express-beans.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.30.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vitest": "^0.0.54",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.30.1",
    "vitest-mock-extended": "^1.1.3",
    "vitest-sonar-reporter": "^0.3.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0"
  }
}
