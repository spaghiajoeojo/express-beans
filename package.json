{
  "name": "express-beans",
  "version": "1.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --runInBand --coverage",
    "generate:docs": "typedoc --out docs src/main.ts",
    "sonar": "sonar-scanner",
    "update": "npx npm-check-updates -u",
    "debug": "npm run coverage --nolazy --detectOpenHandles"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/express-beans.cjs",
  "module": "./dist/express-beans.mjs",
  "exports": {
    ".": {
      "import": "./dist/express-beans.mjs",
      "require": "./dist/express-beans.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "ioc",
    "inversion of control",
    "typescript",
    "decorators",
    "dependency injection",
    "singleton",
    "beans",
    "express"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.24.1",
    "@babel/plugin-transform-typescript": "7.24.1",
    "@babel/preset-env": "7.24.3",
    "@babel/preset-typescript": "7.24.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "sonarqube-scanner": "3.3.0",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typedoc": "0.25.12",
    "typescript": "5.4.2",
    "vite": "5.2.2",
    "vite-plugin-dts": "3.7.3"
  },
  "overrides": {
    "semver": "7.5.3",
    "optionator": "0.9.3"
  },
  "dependencies": {
    "express": "4.18.3",
    "pino": "8.19.0",
    "pino-http": "9.0.0",
    "pino-pretty": "11.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
