{
  "name": "express-beans",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "test": "jest --runInBand",
    "coverage": "jest --runInBand --coverage",
    "generate:docs": "typedoc --out docs src/main.ts",
    "sonar": "sonar-scanner",
    "update": "npx npm-check-updates -u",
    "debug": "npm run coverage --nolazy --detectOpenHandles"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "exports": {
    ".": "./dist/main.js"
  },
  "types": "./dist/main.d.ts",
  "keywords": [
    "ioc",
    "inversion of control",
    "typescript",
    "decorators",
    "dependency injection",
    "singleton",
    "beans",
    "express"
  ],
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "sonarqube-scanner": "3.5.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.11",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typedoc": "0.27.9",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "express": "5.0.1",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "pino-pretty": "13.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
