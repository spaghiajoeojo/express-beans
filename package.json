{
  "name": "express-beans",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:cjs && npm run build:mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json && tsc-alias -p tsconfig.mjs.json",
    "lint": "eslint --ignore-pattern='.gitignore' src test",
    "test": "jest --runInBand",
    "coverage": "jest --runInBand --coverage",
    "generate:docs": "typedoc --out docs src/main.ts",
    "sonar": "sonar-scanner",
    "update": "npx npm-check-updates -u",
    "debug": "npm run coverage --nolazy --detectOpenHandles"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/mjs/main.js",
  "module": "./dist/mjs/main.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/main.js",
      "require": "./dist/cjs/main.js",
      "types": "./dist/types/main.d.ts"
    },
    "./cjs": {
      "require": "./dist/cjs/main.js",
      "types": "./dist/types/main.d.ts"
    }
  },
  "types": "./dist/types/main.d.ts",
  "keywords": [
    "ioc",
    "inversion of control",
    "typescript",
    "decorators",
    "dependency injection",
    "singleton",
    "beans",
    "express"
  ],
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@jest/globals": "29",
    "@types/express": "5.0.3",
    "@types/jest": "29",
    "@types/node": "22",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "jest": "29",
    "sonarqube-scanner": "4.3.0",
    "supertest": "7.1.1",
    "ts-jest": "29",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typedoc": "0.28.5",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "express": "5.1.0",
    "pino": "9.7.0",
    "pino-http": "10.5.0",
    "pino-pretty": "13.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
