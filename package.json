{
  "name": "express-beans",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --runInBand --coverage",
    "generate:docs": "typedoc --out docs src/main.ts",
    "sonar": "sonar-scanner",
    "update": "npx npm-check-updates -u"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/express-beans.cjs.js",
  "module": "./dist/express-beans.es.js",
  "exports": {
    ".": {
      "import": "./dist/express-beans.es.js",
      "require": "./dist/express-beans.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "ioc",
    "inversion of control",
    "typescript",
    "decorators",
    "dependency injection",
    "singleton",
    "beans",
    "express"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.16.0",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sonarqube-scanner": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0"
  },
  "overrides": {
    "semver": "^7.5.3",
    "optionator": "^0.9.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.16.0",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3"
  }
}
