{
  "name": "express-beans",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaghiajoeojo/express-beans.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --no-fix src test",
    "test": "jest",
    "coverage": "jest --coverage",
    "generate:docs": "typedoc --out docs src/main.ts",
    "sonar": "sonar-scanner"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/express-beans.cjs.js",
  "module": "./dist/express-beans.es.js",
  "exports": {
    ".": {
      "import": "./dist/express-beans.es.js",
      "require": "./dist/express-beans.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "ioc",
    "inversion of control",
    "typescript",
    "decorators",
    "dependency injection",
    "singleton",
    "beans",
    "express"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sonarqube-scanner": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0"
  }
}
